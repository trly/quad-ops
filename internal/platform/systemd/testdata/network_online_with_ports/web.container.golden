[Unit]
Description=Web server with networks and ports
After=network-online.target
Wants=network-online.target

[Container]
Label=managed-by=quad-ops
Image=nginx:latest
PublishPort=8080:80
Network=frontend.network
NetworkAlias=web

[Service]
Restart=no
TimeoutStartSec=900

[Install]
WantedBy=default.target
